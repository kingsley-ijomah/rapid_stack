.dashboard-container {
  min-height: 100vh;
  background-color: #f5f5f5;
  padding: 2rem;
}
.agents-section {
  max-width: 1000px;
  margin: 3rem auto 0 auto;
}
.agents-section h2 {
  font-size: 1.3rem;
  font-weight: 700;
  color: #222;
  margin-bottom: 2rem;
  letter-spacing: 0.02em;
}
.agents-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
  gap: 2rem;
} 