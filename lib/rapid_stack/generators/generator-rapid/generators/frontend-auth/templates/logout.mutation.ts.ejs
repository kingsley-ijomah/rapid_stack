import gql from 'graphql-tag';

export const LogoutMutation = gql`
  mutation Logout {
    logout {
      data {
        <% responseFields.forEach(function(field) { %><%= field %>
        <% }); %>
      }
      message
      errors
      httpStatus
    }
  }
`;

<% 
function snakeToCamel(str) {
  return str.replace(/_([a-z])/g, function(match, group1) {
    return group1.toUpperCase();
  });
}
%> 